{
  "name": "typescript-express-rest-api",
  "version": "2.0.10",
  "main": "./build/index.js",
  "keywords": [
    "yeoman-generator",
    "typescript",
    "node",
    "nodejs",
    "rest",
    "restfull",
    "api",
    "builder",
    "generator"
  ],
  "description": "This generator will help you to build your own Node.js Express Mongodb API using TypeScript.",
  "author": "Checha Valerii <chechavalera@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChechaValerii/node-typescript-mongodb/issues"
  },
  "homepage": "https://github.com/ChechaValerii/node-typescript-mongodb#readme",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "tsc --skipLibCheck && node ./build/config/server/index.js",
    "ts:watch": "tsc -w",
    "test": "NODE_ENV=test mocha -r ts-node/register test/index.js --exit",
    "typeorm": "ts-node -P ./tsconfig.json $(yarn bin typeorm) -d ./src/config/connection/index.ts",
    "migration:create": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:create",
    "migration:generate": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:generate -d ./src/config/connection/index.ts",
    "migration:up": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:run -d ./src/config/connection/index.ts",
    "migration:down": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:revert -d ./src/config/connection/index.ts"
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "bcrypt": "5.0.1",
    "body-parser": "1.19.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "14.1.0",
    "express": "4.17.2",
    "helmet": "4.6.0",
    "joi": "17.5.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.1.6",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "10.4.0",
    "tsconfig-paths": "^4.1.2",
    "typeorm": "^0.3.11",
    "util": "0.12.4"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/body-parser": "1.19.2",
    "@types/chai": "4.3.0",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/debug": "4.1.7",
    "@types/dotenv": "8.2.0",
    "@types/ejs": "3.1.0",
    "@types/express": "4.17.13",
    "@types/helmet": "4.0.0",
    "@types/joi": "17.2.3",
    "@types/jsonwebtoken": "8.5.5",
    "@types/mongoose": "5.11.97",
    "@types/node": "17.0.9",
    "@types/supertest": "2.0.11",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/yeoman-generator": "5.2.8",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "chai": "4.3.4",
    "eslint": "8.16.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "jsdoc": "3.6.7",
    "mocha": "9.1.4",
    "nodemon": "2.0.15",
    "source-map-loader": "3.0.1",
    "supertest": "6.2.1",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "4.3.0",
    "typescript": "4.5.4"
  }
}
